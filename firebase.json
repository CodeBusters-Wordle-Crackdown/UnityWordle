{
  "hosting": {
    "public": "Build",
    "headers": [
      {
        "source": "**/*.@(data|framework.js|wasm|loader.js)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/octet-stream"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}



