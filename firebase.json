{
  "hosting": {
    "site": "unity-crackdown",
    "public": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/Build/**",
        "destination": "/Build/"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
